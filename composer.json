{
    "name": "luchaninov/csv-file-loader",
    "type": "library",
    "license": "MIT",
    "description": "Load CSV files using PHP generators",
    "keywords": ["csv", "tsv", "loader", "generator"],
    "authors": [
        {
            "name": "Vladimir Luchaninov",
            "homepage": "https://luchaninov.com"
        }
    ],
    "require": {
        "php": ">=8.1"
